<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <h3><%=@song.name%></h3>
    <h3><%=@updated%></h3>
  <h4>By:<%=@song.artist.name%></h4>
  <h4><a href="/artists/<%=@song.artist.slug%>"><%=@song.artist.name%></a></h4>
  <%@song.genres.each do |genre| %>
  <h5><%=genre.name%></h5>
  <h5><a href="/genres/<%=genre.slug%>"><%=genre.name%></a></h5>
  <%end%>
  </body>
</html>
